{
  "name": "udility-typescript",
  "version": "1.0.0",
  "description": "Express Typescript NodeJS Starter with Docker",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect=8181 --respawn --transpile-only src/index.ts",
    "start": "node ./dist/src/index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier -c \"**/*.{js,ts,json}\"",
    "format": "prettier -w \"**/*.{js,ts,json}\"",
    "lint": "eslint --fix ./src/*",
    "install:husky": "husky install",
    "pre-commit": "npm run typecheck && lint-staged && jest"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint",
    "**/*.{js,ts,json}": "prettier --write"
  },
  "keywords": [],
  "author": "binoy638",
  "license": "MIT",
  "devDependencies": {
    "@types/agenda": "^4.1.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@lavaclient/queue": "^2.0.6",
    "@lavaclient/spotify": "^3.0.0",
    "agenda": "^4.3.0",
    "axios": "^0.27.2",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "dotenv": "^14.3.2",
    "fastest-levenshtein": "^1.0.12",
    "lavaclient": "^4.0.8-rc.2",
    "mongoose": "^6.4.6",
    "redis": "^4.0.4",
    "winston": "^3.6.0",
    "wokcommands": "^1.5.3",
    "ytdl-core": "^4.11.0"
  }
}
